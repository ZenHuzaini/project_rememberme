    <form class="navbar-form navbar-right" role="search" method="post" action="/rememberme/carikandiary" >
            <div class="form-group input-group">
               <div class="bs-example">
                 <input type="text" name="formcaridiary" class="typeahead tt-query" autocomplete="off" spellcheck="false" placeholder="cari diary.."></div>  
              <span class="input-group-btn">
                <button class="btn btn-default" type="submit">
                  <span class="glyphicon glyphicon-search"></span>
                </button>
              </span>      
            </div>
          </form>  
        

          <% if(data.length){ 
                 for(var i = 0;i < data.length;i++) { %>

          <div class="row">
            <div class="col-sm-12">
                    <div class="message-item" id="m9">
                        <div class="message-inner">
                            <div class="message-head clearfix">
                                <div class="user-detail" style="color:#809fff">
                                     <h5><%=data[i].JUDUL_STORY %></h5>   
                                    <div class="post-meta">
                                                <span class="glyphicon glyphicon-time" style="color : #809fff "></span> <span class="qa-message-when-data"><%=data[i].TANGGAL_STORY%></span>
                                          
                                        </div>
                                </div>
                            </div>

                            <div class="qa-message-content" style="color : #2f2f04">
                                <%-data[i].STORY_STORY.substr(0,200)%>
                                <span> ... <a href="/rememberme/bacadiary/<%=data[i].ID_STORY%>">
                                <span style="color : #ff4d4d" class="glyphicon glyphicon-book"></span>
                                </a></span>
                                <span>  <a href="/rememberme/editdiary/<%=data[i].ID_STORY%>">
                                <span style="color : #ff4d4d" class="glyphicon glyphicon-edit"></span>
                                </a></span>
                                <span>  <a href="/rememberme/hapusdiary/<%=data[i].ID_STORY%>">
                                <span style="color : #ff4d4d" class="glyphicon glyphicon-trash"></span>
                                </a></span>
                            </div>

                        </div>
                    </div>
            </div>
    </div>

        <% }
            }else{ %>

                <p>Tidak ada Catatan!!!<p>

            <% } %>

      <div align="center"><a href="/rememberme/lihatdiary"><button type="button" class="btn btn-warning btn-lg" align="center">Lihat Semua Diary</button></a></div>